{
    "name": "phile-cms/phile",
    "type": "project",
    "description": "A file-based CMS with a swappable parser, template engine, cache and storage services, error handler, and meta parser.",
    "keywords": ["cms", "markdown", "flat", "file", "twig", "plugins"],
    "homepage": "http://philecms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank NÃ¤gler",
            "email": "mail@naegler.net",
            "homepage": "https://naegler.hamburg",
            "role": "Developer"
        },
        {
            "name": "James Doyle",
            "email": "james2doyle@gmail.com",
            "homepage": "http://ohdoylerules.com/",
            "role": "Developer"
        },
        {
            "name": "Schlaefer",
            "email": "openmail+sourcecode@siezi.com",
            "homepage": "http://siezi.com/",
            "role": "Developer"
        },
        {
            "name": "PhileCMS Community",
            "homepage": "https://github.com/PhileCMS/Phile/graphs/contributors"
        }
    ],
    "config": {
        "vendor-dir": "lib/vendor"
    },
    "autoload": {
        "psr-4": {
            "Phile\\": "lib/Phile"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "PhileTest\\": "tests/Phile"
      }
    },
    "scripts": {
      "test": "phpunit",
      "phpstan": "phpstan"
    },
    "require": {
        "php": ">=7.4.0",
        "phile-cms/plugin-installer-plugin": "dev-php8",
        "twig/twig": "^2.0 || ^3.0",
        "michelf/php-markdown": ">=1.9",
        "phpfastcache/phpfastcache": "^8.0",
        "filp/whoops": "^2.1",
        "psr/http-factory": "^1.0",
        "psr/container": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/simple-cache": "^1.0",
        "symfony/yaml": "^5.0",
        "laminas/laminas-diactoros": "^2.11",
        "laminas/laminas-httphandlerrunner": "^2.0"
    },
    "require-dev": {
        "phing/phing": "^2.14",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpstan/phpstan": "^0.12.32"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/kicken/ComposerPluginInstaller"
      }
    ]
}
